
<% if(cook==1){ %> 
    <div class="stab">
        <div class="s1">
            <div class="s11">
      <h1>Request a Callback</h1>
            </div>
            <div class="s12">
                <button class="but">X</button>
            </div>
        </div>
        <div class="s2">
            <form action="/home" method="post">
            <div>
                <input class="nam" type="text" placeholder="Name" name="name">
                <input class="pho" type="text" placeholder="Phone no" name="number">
            </div>
            <input class="three" type="text" placeholder="E-mail" name="email">
            <button>SEND</button>
            </form>
        </div>

    </div>
    <% } %> 
    <script>
       document.querySelector('.but').addEventListener('click',function(){
        document.querySelector('.stab').style.visibility='hidden'
       })
        setTimeout(() => {
            document.querySelector('.stab').style.visibility='visible'
        },1000);
    </script>
    <style>
        .three{
            width: 88%;
            height: 40px;
            border: 1px solid black;
            border-radius: 5px;
            margin-left: 6%;
            margin-top: 20px;
        /* box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19); */

        }
        .s2 button{
            width: 150px;
            height: 40px;
            margin-left: 6%;
            margin-top: 20px;
            border: none;
            border-radius: 5px;
            background-color: #003663;
            color: white;
        }
        .s2{
            display: flex;
            flex-direction: column;
            font-family: "Playfair Display", serif;
            background-image: url('/assets/images/callback.jpg');
            background-position: center;
  background-repeat: no-repeat;
  background-size: cover;
        }
        .s2 div{
            display: flex;
            flex-direction: row;
            width: 100%;
            height: 25%;
            display: flex;
            justify-content: space-evenly;
            align-items: center;
            margin-top: 40px;
        }
        .s2 div input{
            /* width: 40%; */
            height: 40px;
            border: 1px solid black;
            border-radius: 5px;
        /* box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19); */

        }
        .nam{
            width: 30%;

        }
        .pho{
            width: 50%;
        }
        .s11{
            width: 80%;
            height: 100%;
            display: flex;
            /* justify-content: center; */
            align-items: center;
        }
        .s11 h1{
            color: white;
            font-family: "Playfair Display", serif;
            margin-top: 23px;
            margin-left: 20px;
        }
        .s12{
            width: 20%;
            height: 100%;
            display: flex;
            justify-content: flex-end;
            
        }
        .s12 button{
            background: none;
            border: none;
            color: white;
            height: 40px;
            width: 40px;
            font-size: 30px;
            margin-right: 10px;
            margin-top: 10px;
        }
        
        .stab{
            width: 40%;
            height: 400px;
            margin-left: 30%;
            background-color: white;
            box-shadow: 0 4px 8px 0 #003663, 0 6px 20px 0 #003663;
            visibility: hidden;
            position: fixed;
            z-index: 1;
            margin-top: 140px;
            display: flex;
            flex-direction: column;
        }
        .s1{
            width: 100%;
            height: 15%;
            background-color: #003663;
            display: flex;
            flex-direction: row;
        }
        .s2{
            width: 100%;
            height: 85%;
        }
        @media (max-width: 767px) {
            .stab{
                width: 90%;
                height: 50%;
                margin-left: 5%;
            }
            .nam{
                width: 35%;
            }
            .pho{
                width: 45%;
            }
        }
    </style>

